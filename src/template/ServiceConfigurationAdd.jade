include ./header.jade

h1 Konfiguration erstellen

form(method="POST" action="/ServiceConfigurationAdd?sessionId=#{sessionId}")
	.field
		label Name
		input(type="text" name="name")
	.field
		label Wert
		input(type="text" name="value")
	input(type="hidden" name="serviceId" value=data.service._id)
	.button
		input.innerButton(type="submit" value="Erstellen")